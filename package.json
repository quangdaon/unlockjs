{
	"name": "unlockjs",
	"title": "UnlockJS",
	"version": "1.1.10",
	"description": "Use keyboard keys to their fullest potential!",
	"main": "dist/unlock.js",
	"module": "src/unlock.js",
	"scripts": {
		"test": "karma start ./karma.conf.js --single-run",
		"codacy": "cat ./coverage/report-lcov/lcov.info | codacy-coverage",
		"dev": "webpack-serve --config ./config/webpack.dev.config.js --content ./demo --log-level info",
		"build": "webpack --config ./config/webpack.prod.config.js --progress --hide-modules && pack build",
		"docs": "jsdoc -c .jsdoc.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/quangdaon/unlockjs.git"
	},
	"keywords": [
		"unlock",
		"cheats",
		"cheater",
		"cheat",
		"codes",
		"keyboard",
		"shortcuts",
		"hotkeys"
	],
	"author": "Quangdao Nguyen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/quangdaon/unlockjs/issues"
	},
	"homepage": "https://docs.quangdao.com/unlockjs/",
	"@pika/pack": {
		"pipeline": [
			[
				"@pika/plugin-standard-pkg"
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.4.0",
		"@babel/plugin-proposal-class-properties": "^7.4.0",
		"@babel/preset-env": "^7.4.2",
		"@pika/pack": "^0.3.7",
		"@pika/plugin-standard-pkg": "^0.4.0",
		"babel-eslint": "^10.0.1",
		"babel-loader": "^8.0.5",
		"babel-plugin-istanbul": "^5.1.1",
		"chai": "^4.2.0",
		"chai-subset": "^1.6.0",
		"codacy-coverage": "^3.4.0",
		"eslint": "^5.15.3",
		"eslint-loader": "^2.1.2",
		"jsdoc": "^3.6.2",
		"karma": "^4.0.1",
		"karma-chai": "^0.1.0",
		"karma-chai-subset": "^2.0.1",
		"karma-coverage": "^1.1.2",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.1.0",
		"karma-mocha": "^1.3.0",
		"karma-sinon": "^1.0.5",
		"karma-sinon-chai": "^2.0.2",
		"karma-webpack": "^3.0.5",
		"mocha": "^5.2.0",
		"sinon": "^7.3.1",
		"sinon-chai": "^3.3.0",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.3.0",
		"webpack-serve": "^2.0.3",
		"yargs": "^12.0.5"
	},
	"dependencies": {}
}
