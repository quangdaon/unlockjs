{
	"name": "unlockjs",
	"title": "UnlockJS",
	"version": "1.1.17",
	"description": "Use keyboard keys to their fullest potential!",
	"main": "dist/unlock.js",
	"module": "src/index.js",
	"scripts": {
		"test": "karma start ./karma.conf.js --single-run",
		"codacy": "cat ./coverage/report-lcov/lcov.info | codacy-coverage",
		"dev": "webpack-dev-server --config ./config/webpack.dev.config.js",
		"build": "webpack --config ./config/webpack.prod.config.js --progress --hide-modules && pack build",
		"docs": "jsdoc -c .jsdoc.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/quangdaon/unlockjs.git"
	},
	"keywords": [
		"unlock",
		"cheats",
		"cheater",
		"cheat",
		"codes",
		"keyboard",
		"shortcuts",
		"hotkeys"
	],
	"author": "Quangdao Nguyen",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/quangdaon/unlockjs/issues"
	},
	"homepage": "https://docs.quangdao.com/unlockjs/",
	"@pika/pack": {
		"pipeline": [
			[
				"@pika/plugin-standard-pkg"
			]
		]
	},
	"devDependencies": {
		"@babel/core": "^7.7.5",
		"@babel/plugin-proposal-class-properties": "^7.7.4",
		"@babel/preset-env": "^7.7.6",
		"@pika/pack": "^0.3.7",
		"@pika/plugin-standard-pkg": "^0.4.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-plugin-istanbul": "^5.2.0",
		"chai": "^4.2.0",
		"chai-subset": "^1.6.0",
		"codacy-coverage": "^3.4.0",
		"eslint": "^5.16.0",
		"eslint-loader": "^2.2.1",
		"jsdoc": "^3.6.3",
		"karma": "^4.4.1",
		"karma-chai": "^0.1.0",
		"karma-chai-subset": "^2.0.1",
		"karma-coverage": "^2.0.1",
		"karma-coveralls": "^2.1.0",
		"karma-firefox-launcher": "^1.2.0",
		"karma-mocha": "^1.3.0",
		"karma-sinon": "^1.0.5",
		"karma-sinon-chai": "^2.0.2",
		"karma-webpack": "^3.0.5",
		"mocha": "^5.2.0",
		"sinon": "^7.5.0",
		"sinon-chai": "^3.3.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^4.8.0",
		"webpack-dev-server": "^4.0.0",
		"webpack-serve": "^3.2.0",
		"yargs": "^12.0.5"
	}
}
